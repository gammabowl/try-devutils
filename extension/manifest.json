{
  "manifest_version": 3,
  "name": "TryDevUtils",
  "description": "Essential developer utilities",
  "version": "0.1.0",
  "homepage_url": "https://www.trydevutils.com",
  "action": {
    "default_title": "TryDevUtils"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
